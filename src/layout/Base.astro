---
import "../styles/global.css";
interface Props {
  title: string;
}

const { title } = Astro.props;
import '@fontsource-variable/open-sans';
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>{title}</title>
    <meta name="viewport" content="width=device-width" />
  </head>
  <body>
    <slot />
  </body>
</html>